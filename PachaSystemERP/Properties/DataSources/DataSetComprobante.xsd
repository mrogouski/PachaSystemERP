                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Comprobante.PuntoVenta, Comprobante.NumeroComprobante, Comprobante.CAE, Comprobante.FechaVencimientoCAE, Comprobante.FechaComprobante, 
                         Comprobante.ImporteTotal, Comprobante.ImporteNetoNoGravado, Comprobante.ImporteNeto, Comprobante.ImporteExento, Comprobante.ImporteTotalTributo, 
                         Comprobante.ImporteTotalIva, Cliente.RazonSocial, Cliente.NumeroDocumento, Cliente.Domicilio, DetalleComprobante.Cantidad, 
                         DetalleComprobante.BaseImponible, DetalleComprobante.Subtotal, DetalleComprobante.ImporteIva, TipoResponsable.Descripcion AS TipoResponsable, 
                         Producto.Codigo, Producto.Descripcion AS Producto, Producto.PrecioUnitario, TipoDocumento.Descripcion AS TipoDocumento, Iva.Descripcion AS Iva, 
                         Iva.Alicuota AS AlicuotaIva
FROM            Comprobante LEFT OUTER JOIN
                         Cliente ON Comprobante.ID = Cliente.ID LEFT OUTER JOIN
                         DetalleComprobante ON Comprobante.ID = DetalleComprobante.ComprobanteID LEFT OUTER JOIN
                         Producto ON DetalleComprobante.ProductoID = Producto.ID LEFT OUTER JOIN
                         Iva ON Producto.IvaID = Iva.ID LEFT OUTER JOIN
                         TipoDocumento ON Cliente.TipoDocumentoID = TipoDocumento.ID LEFT OUTER JOIN
                         TipoResponsable ON Cliente.TipoResponsableID = TipoResponsable.ID
WHERE        (Comprobante.ID = @id)</CommandText>
