                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Comprobante.PuntoVenta, Comprobante.NumeroComprobante, Comprobante.CAE, Comprobante.FechaVencimientoCAE, Comprobante.FechaComprobante, 
                         Comprobante.ImporteTotal, Comprobante.ImporteNetoNoGravado, Comprobante.ImporteNeto, Comprobante.ImporteExento, Comprobante.ImporteTotalTributo, 
                         Comprobante.ImporteTotalIva, Cliente.RazonSocial, Cliente.NumeroDocumento, Cliente.Domicilio, CategoriaTributo.Descripcion, DetalleComprobante.Cantidad, 
                         DetalleComprobante.BaseImponible, DetalleComprobante.Subtotal, DetalleComprobante.ImporteIva, DetalleComprobante.ImporteTributo, 
                         TipoResponsable.Descripcion AS TipoResponsable, Producto.Codigo, Producto.Descripcion AS Producto, Producto.PrecioUnitario, 
                         TipoDocumento.Descripcion AS TipoDocumento, TipoCondicionIva.Descripcion AS TipoCondicionIva, TipoComprobante.Descripcion AS TipoComprobante, 
                         TipoTributo.Descripcion AS TipoTributo, TipoTributo.Alicuota, Comprobante.ID, TipoCondicionIva.Alicuota AS AlicuotaIva
FROM            Comprobante LEFT OUTER JOIN
                         CategoriaTributo ON Comprobante.ID = CategoriaTributo.ID LEFT OUTER JOIN
                         Cliente ON Comprobante.ID = Cliente.ID LEFT OUTER JOIN
                         DetalleComprobante ON Comprobante.ID = DetalleComprobante.ComprobanteID LEFT OUTER JOIN
                         Producto ON DetalleComprobante.ProductoID = Producto.ID LEFT OUTER JOIN
                         TipoComprobante ON Comprobante.TipoComprobanteID = TipoComprobante.ID LEFT OUTER JOIN
                         TipoCondicionIva ON Producto.TipoCondicionIvaID = TipoCondicionIva.ID LEFT OUTER JOIN
                         TipoDocumento ON Cliente.TipoDocumentoID = TipoDocumento.ID LEFT OUTER JOIN
                         TipoResponsable ON Cliente.TipoResponsableID = TipoResponsable.ID LEFT OUTER JOIN
                         TipoTributo ON CategoriaTributo.ID = TipoTributo.CategoriaTributoID
WHERE        (Comprobante.ID = @id)</CommandText>
